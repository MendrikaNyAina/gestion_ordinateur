<?xml version="1.0" encoding="UTF-8"?>
<web-fragment xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-fragment_4_0.xsd"
              version="4.0">

    <jsp-config>
        <taglib>
            <taglib-uri>http://www.front.org/html.tags</taglib-uri>
            <taglib-location>/META-INF/HtmlTag.tld</taglib-location>
        </taglib>
        <taglib>
            <taglib-uri>http://www.front.org/page.tags</taglib-uri>
            <taglib-location>/META-INF/PageTag.tld</taglib-location>
        </taglib>
        <!-- Ajouter d'autres taglibs ici si nÃ©cessaire -->
    </jsp-config>

</web-fragment>
